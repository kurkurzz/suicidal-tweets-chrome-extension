{
	"manifest_version": 3,
	"name": "Suicidal Tweets Detector",
	"description": "Mark potential suicidal tweets on Twitter.",
	"version": "1.0.0",
	"icons": {
		"16": "/icons/16.png",
		"48": "/icons/48.png",
		"128": "/icons/128.png"
	},
	"action": {
		"default_icon": {
			"16": "/icons/16.png",
			"48": "/icons/48.png",
			"128": "/icons/128.png"
		}
	},

	"content_scripts": [
		{
			"matches": ["*://*.twitter.com/*"],
			"js": ["main.js"]
		}
	],

	"author": "Muhammad Nurhafiz"
}